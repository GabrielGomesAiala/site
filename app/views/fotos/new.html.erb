

<section id="pricing">
		<div class="container">
			<div class="row">
				<div class="col-md-12">
					<div class="section-title">
						<h1>Novo Produto</h1>
						<span class="st-border"></span>
					</div>
				</div>

				<div class="col-sm-12">
					<div class="st-pricing text-center">
            <%= form_for @image, :html => {multipart: true}  do |f| %>
						<ul>
              <li><%= f.file_field :imagem, class:"filestyle" %></li>
						</ul>

            <%= f.submit "Create", class:"btn btn-send" %>
					</div>
				</div>
        <% end %>
			</div>
		</div>
	</section>
