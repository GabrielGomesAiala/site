<div class="create">
  <div class="container">
   	<%= form_for @image, :html => {multipart: true}  do |f| %>

      <div class="field">
        <%= f.label :imagem %>
    		<%= f.file_field :imagem %>
      </div>


  		<div class="actions">
    		<%= f.submit "Create" %>
  		</div>
		<% end %>
  </div>
</div>
